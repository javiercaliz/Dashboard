<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
	xmlns:smartTable="sap.ui.comp.smarttable"
	xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="sap.f.Dashboard.controller.ProfessorsController"
	height="100%">

	<Page
		title="Professors"
		showNavButton="true"
		navButtonPress="onNavBack"
		navButtonTooltip="Back">

		<!-- use this to make the table occupy the available screen height -->
		<content>
			<Table id="professorsTable" >
				<columns>
					<Column >
						<Text text="Nombre"/>
					</Column>
					<Column >
						<Text text="Apellido"/>
					</Column>
					<Column >
						<Text text="Edad"/>
					</Column>
					<Column >
						<Text text="Compromiso de Desempeno"/>
					</Column>
					<Column >
						<Text text="Cantidad de Posgrados"/>
					</Column>
					<Column >
						<Text text="Semáforo"/>
					</Column>
					<Column >
						<Text text="Nivel de Inglés"/>
					</Column>
				</columns>
			</Table>
		</content>

	</Page>
</mvc:View>
